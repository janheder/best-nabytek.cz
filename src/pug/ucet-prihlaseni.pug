doctype html
include partials/_variables.pug  
html(lang=lang)
    include partials/_header.pug  
    body
        include partials/_nav.pug
        section.page-heading.post__image-container
            .container
                h1.page-heading__title Uživatelský účet
            img.lazy.post__featured-image(src=URLbase+"img/lazy/lazy-user-thumb-large.jpg" data-src=URLbase+"img/user-thumb-large.jpg" alt="#" title="#")
                
        include partials/_breadcrumb.pug
        main
            .container
                .print-area
                    article.page
                        .page-message
                            h3.page-message__title  Přihlášení bylo úspěšné.
                            .page-message__buttons
                                a.btn.page-message__btn(href="#") Začít nakupovat
                                a.btn.btn-ghost.page-message__btns(href="#") Detail účtu

     

        include partials/_footer.pug

        include partials/_js-include.pug